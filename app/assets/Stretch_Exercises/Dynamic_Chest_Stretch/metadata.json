{
    "title": "Dynamic Chest Stretch",
    "description": "A stretch exercise.",
    "difficulty": "Beginner",
    "content": [
        {
            "type": "section",
            "title": "Instructions",
            "content": {
                "type": "steps",
                "items": [
                    "Stand with your feet shoulder-width apart and your arms extended out to your sides at shoulder height.",
                    "Slowly bring your arms forward, crossing them in front of your body.",
                    "As you cross your arms, squeeze your chest muscles and hold for 10-15 seconds.",
                    "Release the stretch and return your arms to the starting position.",
                    "Repeat the stretch for 2-3 sets of 10-15 repetitions."
                ]
            }
        },
        {
            "type": "section",
            "title": "Benefits",
            "content": {
                "type": "list",
                "items": [
                    "Increases flexibility and range of motion in the chest muscles",
                    "Helps to prevent and alleviate shoulder and neck pain",
                    "Improves posture by opening up the chest and shoulders",
                    "Can be done anywhere without any equipment",
                    "Can be modified to target specific areas of the chest and shoulders"
                ]
            }
        },
        {
            "type": "section",
            "title": "Primary Muscles",
            "content": {
                "type": "list",
                "items": [
                    "Pectoralis Major",
                    "Pectoralis Minor",
                    "Anterior Deltoids",
                    "Serratus Anterior"
                ]
            }
        },
        {
            "type": "section",
            "title": "Variations",
            "content": {
                "type": "list",
                "items": [
                    "Standing Dynamic Chest Stretch",
                    "Seated Dynamic Chest Stretch",
                    "Wall Dynamic Chest Stretch",
                    "Partner Dynamic Chest Stretch",
                    "Resistance Band Dynamic Chest Stretch"
                ]
            }
        }
    ]
}