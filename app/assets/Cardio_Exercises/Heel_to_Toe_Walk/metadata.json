{
    "title": "Heel to Toe Walk",
    "description": "A cardio exercise.",
    "difficulty": "Beginner",
    "content": [
        {
            "type": "section",
            "title": "Instructions",
            "content": {
                "type": "steps",
                "items": [
                    "Stand with your feet together and your arms at your sides.",
                    "Take a step forward with your right foot, placing your heel just in front of the toes of your left foot.",
                    "Take another step with your left foot, placing your heel just in front of the toes of your right foot.",
                    "Continue walking in this heel-to-toe fashion for 20 steps.",
                    "Turn around and walk back in the same manner for another 20 steps."
                ]
            }
        },
        {
            "type": "section",
            "title": "Benefits",
            "content": {
                "type": "list",
                "items": [
                    "Improves cardiovascular health",
                    "Increases endurance and stamina",
                    "Helps with weight loss and management",
                    "Strengthens bones and muscles",
                    "Improves coordination and balance",
                    "Can be done anywhere with minimal equipment",
                    "Can be a fun and social activity"
                ]
            }
        },
        {
            "type": "section",
            "title": "Primary Muscles",
            "content": {
                "type": "list",
                "items": [
                    "Gastrocnemius",
                    "Soleus",
                    "Tibialis anterior",
                    "Quadriceps",
                    "Hamstrings",
                    "Glutes",
                    "Abdominals",
                    "Lower back muscles"
                ]
            }
        },
        {
            "type": "section",
            "title": "Variations",
            "content": {
                "type": "list",
                "items": [
                    "Heel-to-toe walk with eyes closed",
                    "Heel-to-toe walk on a balance beam",
                    "Heel-to-toe walk while holding a weight",
                    "Heel-to-toe walk on an unstable surface",
                    "Heel-to-toe walk backwards"
                ]
            }
        }
    ]
}