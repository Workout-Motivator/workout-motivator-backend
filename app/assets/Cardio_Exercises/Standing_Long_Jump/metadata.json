    "difficulty": "Beginner",
    "content": [
        {
            "type": "section",
            "title": "Instructions",
            "content": {
                "type": "steps",
                "items": [
                    "Stand with your feet shoulder-width apart.",
                    "Bend your knees and lower your hips, keeping your back straight.",
                    "Swing your arms back behind you.",
                    "Jump forward as far as you can, swinging your arms forward to help propel you.",
                    "Land on both feet, with your knees slightly bent to absorb the impact.",
                    "Measure the distance you jumped from the starting line.",
                    "Repeat for desired number of jumps."
                ]
            }
        },
